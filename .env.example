# Base RPC URL
BASE_RPC_URL=https://mainnet.base.org

# Your wallet private key (WITHOUT 0x prefix)
PRIVATE_KEY=your_private_key_here

# Number of times to mint
MINT_COUNT=5

# Optional: Alchemy API key for instant NFT lookup (like OpenSea)
# Get free key at https://www.alchemy.com/
# ALCHEMY_API_KEY=your_alchemy_key_here

# Transfer settings (optional)
# Enable automatic transfer of minted NFTs to other addresses
TRANSFER_ENABLED=false

# Comma-separated list of recipient addresses (one NFT per address in order)
# Example: 0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb1,0x123...abc,0x456...def
TRANSFER_ADDRESSES=
